<h1>Movie List list</h1>
<%= link_to 'Create a list', new_list_path %>
<% @lists.each do |list| %>
<ul>
  <li>
    <%= link_to list.name, list_path(list) %>
    <%= link_to 'Edit', edit_list_path(list) %>
    <%= link_to 'Delete', list_path(list), method: :delete, data: { confirm: "Are you sure?" } %>
  </li>
</ul>
<% end %>
